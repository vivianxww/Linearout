<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.admin.myapplication.Main3Activity"
    android:background="@color/abc_input_method_navigation_guard"
    android:weightSum="1">


    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="false"
        android:id="@+id/linearLayout">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" One, One "
            android:id="@+id/textView11"
            android:textColor="@color/abc_hint_foreground_material_dark"
            android:textSize="25dp"
            android:background="@drawable/tv_border" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="  One, Two "
            android:id="@+id/textView12"
            android:textColor="@color/abc_hint_foreground_material_dark"
            android:textSize="25dp"
            android:background="@drawable/tv_border"
            android:layout_marginLeft="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="  One, Three "
            android:id="@+id/textView13"
            android:textSize="25dp"
            android:textColor="@color/abc_hint_foreground_material_dark"
            android:background="@drawable/tv_border"
            android:singleLine="true"
            android:layout_marginLeft="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="   One, Four  "
            android:id="@+id/textView14"
            android:textColor="@color/abc_hint_foreground_material_dark"
            android:textSize="25dp"
            android:background="@drawable/tv_border"
            android:layout_marginLeft="5dp" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout"
        android:layout_alignParentStart="true"
        android:id="@+id/linearLayout3">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" Two, One  "
            android:id="@+id/textView15"
            android:textColor="@color/abc_hint_foreground_material_dark"
            android:textSize="25dp"
            android:background="@drawable/tv_border"
            android:layout_marginTop="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" Two, Two  "
            android:id="@+id/textView16"
            android:textColor="@color/abc_hint_foreground_material_dark"
            android:textSize="25dp"
            android:background="@drawable/tv_border"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" Two, Three  "
            android:id="@+id/textView17"
            android:textColor="@color/abc_hint_foreground_material_dark"
            android:textSize="25dp"
            android:background="@drawable/tv_border"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" Two, Four  "
            android:id="@+id/textView18"
            android:textColor="@color/abc_hint_foreground_material_dark"
            android:textSize="25dp"
            android:background="@drawable/tv_border"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout3"
        android:layout_alignParentStart="true"
        android:id="@+id/linearLayout2">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Three,One"
            android:id="@+id/textView19"
            android:textColor="@color/abc_hint_foreground_material_dark"
            android:textSize="25dp"
            android:background="@drawable/tv_border"
            android:layout_below="@+id/linearLayout3"
            android:layout_marginTop="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" Three, Two"
            android:id="@+id/textView20"
            android:layout_marginLeft="5dp"
            android:background="@drawable/tv_border"
            android:textColor="@color/abc_hint_foreground_material_dark"
            android:textSize="25dp"
            android:layout_alignTop="@+id/textView19"
            android:layout_toEndOf="@+id/textView19"
            android:layout_marginTop="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" Three,Three"
            android:id="@+id/textView21"
            android:layout_alignTop="@+id/textView20"
            android:layout_toEndOf="@+id/textView20"
            android:layout_marginLeft="5dp"
            android:background="@drawable/tv_border"
            android:textIsSelectable="false"
            android:textSize="25dp"
            android:textColor="@color/abc_hint_foreground_material_dark"
            android:layout_marginTop="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Three,Four"
            android:id="@+id/textView22"
            android:layout_alignTop="@+id/textView21"
            android:layout_toEndOf="@+id/textView21"
            android:textColor="@color/abc_hint_foreground_material_dark"
            android:textSize="25dp"
            android:background="@drawable/tv_border"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout2"
        android:layout_alignParentStart="true"
        android:layout_marginTop="41dp">

    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Four, One"
        android:id="@+id/textView23"
        android:background="@drawable/tv_border"
        android:textColor="@color/abc_hint_foreground_material_dark"
        android:textSize="25dp"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Four, Two "
        android:id="@+id/textView24"
        android:textColor="@color/abc_hint_foreground_material_dark"
        android:textSize="25dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/tv_border"
        android:layout_below="@+id/linearLayout2"
        android:layout_toEndOf="@+id/textView23"
        android:layout_marginTop="5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Four , Three"
        android:id="@+id/textView25"
        android:textColor="@color/abc_hint_foreground_material_dark"
        android:textSize="25dp"
        android:background="@drawable/tv_border"
        android:layout_marginLeft="5dp"
        android:layout_below="@+id/linearLayout2"
        android:layout_toEndOf="@+id/textView24"
        android:layout_marginTop="5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Four,  Four"
        android:id="@+id/textView26"
        android:layout_marginLeft="5dp"
        android:background="@drawable/tv_border"
        android:textSize="25dp"
        android:textColor="@color/abc_hint_foreground_material_dark"
        android:layout_below="@+id/linearLayout2"
        android:layout_toEndOf="@+id/textView25"
        android:layout_marginTop="5dp" />

</RelativeLayout>
